			},error:function(){
				console.log("즐겨찾기 제이슨 에러다");
			}
		});
	}
	
	function uploadCnt(){
		var cnt = $("#fileCnt").val();
		var fileupload = $("#fileupload");
		
		if(cnt == ''){
			alert("첨부할 파일 개수를 입력해주세요");
			return;
		}
		
		$(fileupload).html("");
		
		for(var i=0;i<cnt;i++){
			var inputFile = $("<p><input type='file' name='file' required></p>");
			fileupload.append(inputFile);
		}
	}
	
